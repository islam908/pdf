{
    "name": "pdf-protector/tool",
    "description": "أداة حماية ملفات PDF بكلمة مرور",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "PDF Protector Team",
            "email": "info@pdfprotector.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "setasign/fpdi": "^2.3",
        "tecnickcom/tcpdf": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "PdfProtector\\": "src/"
        }
    },
    "scripts": {
        "install-deps": "composer install --no-dev --optimize-autoloader",
        "update-deps": "composer update --no-dev --optimize-autoloader"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "minimum-stability": "stable"
}